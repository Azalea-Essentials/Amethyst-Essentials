{
  "format_version": "1.12.0",
  "render_controllers": {
    "controller.render.crate": {
      "geometry": "Array.geo[query.skin_id]",
      "materials": [
        {
          "*": "Material.default"
        }
      ],
      "textures": [
        "Array.skins[query.skin_id]"
      ],
      "arrays": {
        "textures": {
          "Array.skins": [
            "Texture.default",
            "Texture.normal",
            "Texture.custom"
          ]
        },
        "geometries": {
          "Array.geo": [
            "Geometry.default",
            "Geometry.change",
            "Geometry.custom"
          ]
        }
      }
    }
  }
}